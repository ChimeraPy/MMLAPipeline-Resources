{
  "mode": "record",
  "workers": {
    "manager_ip": "10.66.191.251",
    "manager_port": 9001,
    "instances": [
      {
        "name": "local",
        "id": "local",
        "description": "local worker for the MMLA pipeline demo with a video node"
      }
    ]
  },
  "nodes": [
    {
      "registry_name": "MMLAPIPE_Video",
      "name": "camera",
      "kwargs": {
        "video_src": "https://vanderbilt.box.com/shared/static/pn2usc5wdfjp1azx37dxz2h3ba85sicw.mp4",
        "width": 800,
        "frame_key": "frame",
        "include_meta": true,
        "download_video": true
      },
    "package": "mmlapipe"
    },
    {
      "registry_name": "MMLAPIPE_GazeL2CSNet",
      "name": "gaze-l2cs-net",
      "kwargs": {
        "weights": "https://vanderbilt.box.com/shared/static/3n5m0upd9n87x7lkrusbe0rh5hkvsgp2.pkl",
        "device": "cuda",
        "frames_key": "frame",
        "show": true
      },
      "package": "mmlapipe"
    },
    {
      "registry_name": "MMLAPIPE_Video",
      "name": "screen-capture",
      "kwargs": {
        "video_src": "https://vanderbilt.box.com/shared/static/5stwbljk2jfliyst7886oa11w7moi9or.mp4",
        "width": 1920,
        "height": 1080,
        "frame_key": "frame",
        "include_meta": true,
        "download_video": true
      },
    "package": "mmlapipe"
    },
    {
      "registry_name": "MMLAPIPE_LogReader",
      "name": "logs",
      "kwargs": {
        "logfile": "https://vanderbilt.box.com/shared/static/vomwckyroqygmcrkjoezi82hyt5olukx.csv",
        "data_key": "data",
        "batch_window_size": 0.25,
        "timestamp_column": "timestamp",
        "timestamp_format": "%H:%M:%S:%f",
        "offset": 518.5
      },
    "package": "mmlapipe"
    },
    {
      "registry_name": "MMLAPIPE_GEMSTEPLogProcessor",
      "name": "log-processor",
      "kwargs": {
        "data_key": "data",
        "frame_key": "frame",
        "show": true
      },
    "package": "mmlapipe"
    }
  ],
  "adj": [
    [
      "camera",
      "gaze-l2cs-net"
    ],
    [
      "screen-capture",
      "log-processor"
    ],
    [
      "logs",
      "log-processor"
    ]
  ],
  "manager_config": {
    "logdir": "cp-logs",
