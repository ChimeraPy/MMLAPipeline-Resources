  "mappings": {
    "local": [
      "screen-capture",
      "camera",
      "log-processor",
      "logs",
      "gaze-l2cs-net"
    ]
  },
  "timeouts": {
    "commit_timeout": 300,
    "shutdown_timeout": 300
  }
